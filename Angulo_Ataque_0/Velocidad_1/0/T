FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293;

boundaryField
{
    // Coincide con blockMeshDict: aerofoil
    aerofoil
    {
        type            zeroGradient; // Adiab√°tico
    }

    // Coincide con blockMeshDict: inlet
    inlet
    {
        type            freestream;
        freestreamValue uniform 293;
        value           uniform 293;
    }

    // Coincide con blockMeshDict: outlet
    outlet
    {
        type            freestream;
        freestreamValue uniform 293;
        value           uniform 293;
    }

    frontAndBack
    {
        type            empty;
    }
}
